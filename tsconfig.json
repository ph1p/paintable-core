{
  "compilerOptions": {
    "target": "es6",
    "outDir": "./dist",
    "moduleResolution": "node",
    "lib": ["es2016", "dom", "es5"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationDir": "./"
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
